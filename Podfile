  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for 7Eleven
  pod 'ADALiOS'

  target '7ElevenTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target '7ElevenUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
